{
	"$schema": "https://turbo.build/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [
				"dist/**",
				".vercel/**",
				".nitro/**",
				".output/**",
				".tanstack/**"
			],
			"env": ["SERVER_*", "VITE_*"]
		},
		"lint": {
			"dependsOn": ["^lint"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"inputs": ["$TURBO_DEFAULT$", ".env"],
			"cache": false,
			"persistent": true
		}
	}
}
